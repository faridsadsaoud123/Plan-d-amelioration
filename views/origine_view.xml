<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="pdca_origine_view_form" model="ir.ui.view">
        <field name="name">pdca.origine.view.form</field>
        <field name="model">pdca.origine</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="pdca_origine_view_tree" model="ir.ui.view">
        <field name="name">pdca.origine.view.tree</field>
        <field name="model">pdca.origine</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="pdca_origine_action" model="ir.actions.act_window">
        <field name="name">Origine</field>
        <field name="res_model">pdca.origine</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="pdca_origine_menu" name="Origine"
        action="pdca_origine_action" parent="pdca_root_menu"
        sequence="40"/>
</odoo>
